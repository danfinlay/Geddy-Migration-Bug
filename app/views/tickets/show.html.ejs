<div class="hero-unit">
  <h2 class="id"><%= ticket.id %></h2>
  <%- linkTo('Edit this ticket', editTicketPath(params.id), {class: 'btn pull-right'}); %>
  <%- linkTo('Back to list', ticketPath(''), {class: 'btn pull-right'}); %>
</div>

<h2>Ticket Properties</h2>
<% for(var i in ticket) { -%>
<% if(i !== 'saved') { -%>
<div class="row list-item">
  <div class="span4">
    <h3><%= i %></h3>
  </div>
  <div class="span8">
    <h3><i class="icon-list-alt"></i><span class="<%= i %>"><%= ticket[i] %></span></h3>
  </div>
</div>
<% } -%><% } -%>